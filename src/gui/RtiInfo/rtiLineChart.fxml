<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainBox" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RtiInfo.LineChartController">
    <TilePane prefHeight="700.0" prefWidth="1200.0">
        <Pane prefHeight="200.0" prefWidth="200.0">
            <LineChart fx:id="cpuLineChart" prefHeight="350.0" prefWidth="600.0">
                <xAxis>
                    <CategoryAxis side="BOTTOM" />
                </xAxis>
                <yAxis>
                    <NumberAxis side="LEFT" />
                </yAxis>
            </LineChart>
        </Pane>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <LineChart fx:id="memLineChart" prefHeight="350.0" prefWidth="600.0">
                <xAxis>
                    <CategoryAxis side="BOTTOM" />
                </xAxis>
                <yAxis>
                    <NumberAxis side="LEFT" />
                </yAxis>
            </LineChart>
        </Pane>
        <Pane prefHeight="350.0" prefWidth="600.0">
            <LineChart fx:id="netLineChart" layoutX="21.0" prefHeight="350.0" prefWidth="600.0">
                <xAxis>
                    <CategoryAxis side="BOTTOM" />
                </xAxis>
                <yAxis>
                    <NumberAxis side="LEFT" />
                </yAxis>
            </LineChart>
        </Pane>
        <Pane prefHeight="200.0" prefWidth="200.0">
            <LineChart fx:id="diskLineChart" prefHeight="350.0" prefWidth="600.0">
                <xAxis>
                    <CategoryAxis side="BOTTOM" />
                </xAxis>
                <yAxis>
                    <NumberAxis side="LEFT" />
                </yAxis>
            </LineChart>
        </Pane>
    </TilePane>
</VBox>
